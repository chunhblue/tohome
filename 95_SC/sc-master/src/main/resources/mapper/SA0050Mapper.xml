    MA1000.ofc,
    t_operator.c_operatorname AS ofc_name,
    cashier_id,
    cashier_name,
    duty,
    (select code_name from cm9010 where cm9010.code_type='00215' and cm9010.code_value=sa0050.cashier_level)as cashier_level,
    sa0050.cashier_level as cashier_level_cd,
    cashier_password as password,
    effective_sts,
    (select code_name from cm9010 where cm9010.code_type='00220' and cm9010.code_value=sa0050.effective_sts) as effective_sts_name,
    sa0050.store_cd,
    ma1000.store_name,
    sa0050.cashier_email
    from sa0050 left join ma1000
    on
    sa0050.store_cd = ma1000.store_cd
    left join  t_operator on t_operator.c_operatorcode=ma1000.ofc
    where 1=1
    AND sa0050.store_cd IN
